  },
  "homepage": "https://github.com/diegomdrs/gerador-lista-artefato-qas#readme",
  "dependencies": {
    "angular": "^1.7.9",
    "angular-block-ui": "^0.2.2",
    "angular-resource": "^1.7.9",
    "angular-route": "^1.7.9",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "commander": "^4.0.1",
    "express": "^4.17.1",
    "ng-device-detector": "^5.1.4",
