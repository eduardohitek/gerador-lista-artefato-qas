{
  "name": "gerador-lista-artefato-qas",
  "version": "0.0.5",
  "description": "Gerador de listagem de artefatos do QAS",
  "bin": {
    "gerador-lista-artefato": "./app.js"
  },
  "pkg": {
    "assets": "./public"
  },
  "scripts": {
    "start": "node app -s",
    "dev": "node --inspect app -s",
    "test": "jest --runInBand --verbose",
    "pkg-win": "pkg . -t node8-win-x64 -o dist/gerador-lista-artefato-qas.exe",
    "pkg-linux": "pkg . -t node8-linux-x64 -o dist/gerador-lista-artefato-qas",
    "pkg": "npm run pkg-win && npm run pkg-linux"
  },
